string problemName = "pb2";

border C(t = 0, 2 * pi) {
    x=cos(t);
    y=sin(t);
    label=1;
}
mesh Mesh = buildmesh(C(1700));

func f = 0;
func uExact = 3 * y * x^2 - y^3;

varf poisson(u, v) = 
    int2d(Mesh)(
        dx(u)*dx(v) + 
        dy(u)*dy(v)
    )
    + int2d(Mesh)(
        f * v
    )
    + on(1, u = sin(3 * atan2(y, x)));